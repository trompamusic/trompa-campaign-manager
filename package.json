{
  "name": "trompa-campaign-manager",
  "description": "This is the frontend application of the TROMPA Campaign Manager",
  "version": "1.3.2",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": "https://github.com/trompamusic/trompa-campaign-manager/issues"
  },
  "keywords": [
    "React",
    "React i18n",
    "React Router",
    "React Redux",
    "React Sagas",
    "React Helmet",
    "React Hooks",
    "React testing library",
    "Reselect",
    "Enzyme",
    "GraphQL",
    "Jest",
    "Material UI",
    "Storybook"
  ],
  "author": "Christiaan Scheermeijer <christiaan@videodock.com>",
  "contributors": [
    "CodeKarl",
    "demmyhonore"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:cc": "npx codeceptjs run --steps -c  codecept.conf.js",
    "test:chrome": "npx codeceptjs run -c codecept.chrome.conf.js",
    "test:firefox": "npx codeceptjs run --steps -c codecept.firefox.conf.js",
    "test:safari": "npx codeceptjs run -c codecept.safari.conf.js",
    "test:local:chrome": "concurrently --kill-others --success first -n \"server,test\" \"npx serve -s build -l 3000 \" \"wait-on http://localhost:3000 && yarn test:chrome\"",
    "test:local:firefox": "concurrently --kill-others --success first -n \"server,test\" \"npx serve -s build -l 3000 \" \"wait-on http://localhost:3000 && yarn test:firefox\"",
    "test:local:safari": "concurrently --kill-others --success first -n \"server,test\" \"npx serve -s build -l 3000 \" \"wait-on http://localhost:3000 && yarn test:safari\"",
    "lint": "eslint --ext jsx --ext js src",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add -A CHANGELOG.md",
    "build-storybook": "build-storybook -s public",
    "storybook": "start-storybook -p 9009 -s public"
  },
  "resolutions": {
    "meld-clients-core": "Videodock/meld-clients-core#dev-meld-2.0-postbuild"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-testing": "^3.1.3",
    "@date-io/moment": "1.x",
    "@material-ui/core": "^4.9.4",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "@material-ui/pickers": "^3.2.10",
    "apisauce": "^2.0.1",
    "apollo-boost": "^0.4.7",
    "aws-sdk": "^2.789.0",
    "classnames": "^2.2.5",
    "codeceptjs": "^3.0.5",
    "codeceptjs-resemblehelper": "^1.9.3",
    "concurrently": "^6.0.0",
    "connected-react-router": "^6.5.0",
    "eslint-plugin-codeceptjs": "^1.3.0",
    "formik": "^2.1.4",
    "graphql": "^14.5.8",
    "history": "^4.7.2",
    "i18next": "^17.0.6",
    "meld-clients-core": "Videodock/meld-clients-core#dev-meld-2.0-postbuild",
    "moment": "^2.19.2",
    "nuka-carousel": "^4.7.1",
    "object-hash": "^1.2.0",
    "playwright": "^1.9.0",
    "prop-types": "^15.6.0",
    "react": "16.13.0",
    "react-app-polyfill": "^1.0.5",
    "react-dom": "16.13.0",
    "react-helmet": "^5.2.0",
    "react-i18next": "^10.11.4",
    "react-jss": "^8.1.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.1",
    "react-typeform-embed": "^0.2.1",
    "redux": "^4.0.5",
    "redux-promise": "^0.6.0",
    "redux-saga": "^1.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.1.2",
    "selectable-score": "trompamusic/selectable-score",
    "solid-auth-client": "^2.5.2",
    "trompa-multimodal-component": "^1.2.1",
    "url-search-params-polyfill": "^7.0.0",
    "wait-on": "^5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^8.0.0",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/addon-storyshots": "^5.1.9",
    "@storybook/react": "^6.1.21",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.0.15",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.10",
    "@types/react-dom": "^16.0.1",
    "@types/seamless-immutable": "^7.1.1",
    "boards": "^3.1.4",
    "boards-preset-vd": "^3.1.0",
    "conventional-changelog": "^3.1.24",
    "conventional-changelog-cli": "^2.1.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.5.4",
    "eslint-config-react-app-vd": "^1.0.4",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-varspacing": "^1.1.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "node-emoji": "^1.8.1",
    "react-test-renderer": "16.9.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint",
      "react-scripts test --watchAll=false --env=jsdom --passWithNoTests --findRelatedTests"
    ]
  },
  "jest": {
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "src/containers/**/*.{js,jsx}",
      "!src/components/Root/**/*.{js,jsx}",
      "!src/components/Icons/**/*.{js,jsx}"
    ]
  },
  "browserslist": {
    "production": [
      ">2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  }
}
